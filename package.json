{
  "name": "@jls/tognox-frontend",
  "version": "0.1.0",
  "scripts": {
    "serve": "NODE_ENV=production node ./build/server/bundle.js",
    "start": "node scripts/start.js",
    "nodemon": "nodemon --watch build/server build/server/bundle.js build/server/bundle.js",
    "build-client": "node scripts/build-client.js",
    "build-server": "node scripts/build-server.js",
    "build": "CI=false npm run build-client && npm run build-server",
    "lint": "eslint 'src/**/*.+(js|jsx)'",
    "test": "node scripts/test.js --env=jsdom"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "camelcase-keys": "^4.2.0",
    "classnames": "^2.2.5",
    "express": "^4.15.3",
    "immutable": "^3.8.2",
    "node-sass": "^4.7.2",
    "nodemailer": "^4.4.1",
    "object-hash": "^1.2.0",
    "prop-types": "^15.5.10",
    "query-string": "^5.0.1",
    "react": "^16.3.2",
    "react-context-component": "^0.0.3",
    "react-dom": "^16.3.2",
    "react-dom-server": "^0.0.5",
    "react-helmet": "^5.2.0",
    "react-parallax": "^1.6.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-slick": "^0.23.1",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "serialize-javascript": "^1.4.0",
    "snakecase-keys": "^1.1.0",
    "source-map-support": "^0.5.0",
    "unescape": "^1.0.1"
  },
  "devDependencies": {
    "autoprefixer": "7.1.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-engine-plugin": "^0.1.2",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-app": "^3.0.0",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "concurrently": "^3.4.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.7",
    "dotenv": "4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.4",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "0.11.1",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.28.0",
    "http-proxy-middleware": "^0.17.4",
    "isomorphic-fetch": "^2.2.1",
    "jest": "20.0.3",
    "nodemon": "^1.11.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.5",
    "promise": "7.1.1",
    "react-dev-utils": "^3.0.0",
    "react-error-overlay": "^1.0.7",
    "redux-mock-store": "^1.5.1",
    "style-loader": "^0.19.1",
    "sw-precache-webpack-plugin": "0.9.1",
    "url-loader": "0.5.8",
    "webpack": "3.4.1",
    "webpack-dev-server": "2.4.5",
    "webpack-manifest-plugin": "1.1.0",
    "webpack-node-externals": "^1.6.0",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.scss$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|scss|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/scripts/setupTests.js",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(jpg|gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
